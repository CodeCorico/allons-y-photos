{
  "env": [{
    "type": "confirm",
    "name": "INDEXER",
    "message": "Enable the Photos indexer process:"
  }, {
    "type": "input",
    "name": "INDEXER_PATH",
    "message": "Path of the Photos to index:",
    "when": "INDEXER=true"
  }, {
    "type": "confirm",
    "name": "INDEXER_CONVERT_MOV",
    "message": "Convert .mov videos to .mp4 format:",
    "default": false,
    "when": "INDEXER=true"
  }]
}
